alias reload='. ~/.zshrc'
alias tat='tmux a -t'
alias tls='tmux ls'

alias rsp='rsync -avz --info=progress2'

alias gs='git status'
alias gssb='git status -sb'
alias ga='git add'
alias gau='git add -u'
alias gc='git commit'
alias gcm='git commit -m'
alias gca='git commit --amend'
alias gls='git log --graph --pretty=format:"%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset" --abbrev-commit --date=relative'
alias glg='git log --color --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset" --abbrev-commit'
alias glol='git log --pretty=oneline --abbrev-commit --graph --decorate'
alias gl='git log'
alias gd='git diff'
gdo() {
    gd HEAD~$1 HEAD
}
alias gp='git push'
alias gpo='git push origin'
alias gpom='git push origin master'
alias gpoc='git push origin $(git symbolic-ref --short HEAD)'
alias gg='git pull'
alias ggo='git pull origin'
alias ggom='git pull origin master'
alias gb='git branch'
alias gbv='git branch -vv'
alias gt='git stash'
alias gtl='git stash list'
alias gtp='git stash pop'
alias gf='git fetch'
alias gfo='git fetch origin'
gtps() {
    git stash pop stash@{$1}
}
alias gco='git checkout'
alias gcob='git checkout -b'
alias gcp='git cherry-pick'

alias nrwd='npm run watch:dev'

alias resetcaps="setxkbmap -option escape:caps"
alias nocaps="setxkbmap -option caps:escape"
alias nobell="xset b off"
alias fr="setxkbmap fr"

alias bright='echo $1 | sudo tee /sys/class/backlight/intel_backlight/brightness'
alias remap='/usr/bin/setxkbmap -option "caps:swapescape"'

# alias vim='nvim'
alias oldvim='/usr/bin/vim'
alias bim='vim'
alias viom='vim'

alias gmail="chromium http://gmail.com"

alias whoisonport80='sudo netstat -tulpn | grep ":80"'
alias whoisonport='sudo netstat -tulpn | grep '

# aliases for docker
alias dc='docker-compose'
alias dcu='docker-compose up -d'
alias dcud='docker-compose up -d'
alias dcs='docker-compose stop'
alias dcd='docker-compose down'
alias dcps='docker-compose ps'
alias dce='docker-compose exec'
dcepb() {
    dce service_$1_php bash
}
dceb() {
    dce service_$1 bash
}
dceab () {
  dce service_advango_$1 bash
}
alias dcr='docker-compose run'
alias dcrd='docker-compose run -d'
alias dcb='docker-compose build'
alias dcall='dcb;dcud;dcps'

# aliases for vagrant
alias vu='vagrant up'

# apache
alias a2start='sudo /etc/init.d/apache2 start'
alias a2stop='sudo /etc/init.d/apache2 stop'
alias a2restart='sudo /etc/init.d/apache2 restart'

# generate ctags
alias ctags='ctags-exuberant -R --PHP-kinds=+cf --exclude=".git" .'
alias ctagsapi='ctags-exuberant -R --PHP-kinds=+cf --exclude=".git" api'


# get pid of window
alias xpropwin="xprop _NET_WM_PID | cut -d' ' -f3"


